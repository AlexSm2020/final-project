    "axios": "^0.19.0",
    "bcryptjs": "2.4.3",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "http-proxy-middleware": "1.0.5",
    "mongoose": "^5.7.9",
    "morgan": "^1.9.1",
